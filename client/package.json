    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-router-dom": "^4.2.2",
    "styled-components": "^1.4.4"
  },
  "devDependencies": {
    "node-sass": "^4.5.2",
    "react-scripts": "0.9.5"
  },
  "scripts": {
